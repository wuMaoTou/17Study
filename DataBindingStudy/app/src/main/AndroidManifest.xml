<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maotou.databindingstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BindingViewIdActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".BindingDataActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BindingModuleActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BindingEventActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BindingStaticCastActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BindingModuleObserverActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BindingMapActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".BindingIncludeActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>